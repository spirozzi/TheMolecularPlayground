</script>


	<script type="text/javascript">
		Info = {
			width: "100%",
			height: "100%",
			color: "white",
			j2sPath: "lib/jsmol/j2s",
			addSelectionOptions: false,
			readyFunction: null,
			console: "jmol_infodiv",
			disableInitialConsole: true,
			defaultModel: null,
			debug: false,
			spin: true
		};
	</script>

	<div id="jmolPrev" ng-show="!logged_in || view == 'Gallery' || view == 'Content Author'">
		<script type="text/javascript">
			jmol = Jmol.getApplet("jmol", Info);
      Jmol.script(jmol, 'load "jsmol/adderall.mol";' );

			load_model = function(modelPath,modelName){
				Jmol.script(jmol, 'load "'+modelPath.innerHTML+'";' );
				document.getElementById('molName').innerHTML = modelName.innerHTML;
			}
		</script>
	</div>
